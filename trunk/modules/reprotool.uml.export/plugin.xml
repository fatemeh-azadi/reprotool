<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         point="org.eclipse.ui.popupMenus">
      <objectContribution
            adaptable="false"
            id="reprotool.uml.export.umlContribution"
            nameFilter="*.swproj"
            objectClass="org.eclipse.core.resources.IFile">
         <action
               class="reprotool.uml.export.actions.Swproj2UMLClass"
               id="reprotool.uml.export.action2"
               label="Convert SW project to UML class diagram">
         </action>
         <action
               class="reprotool.uml.export.actions.Swproj2UMLUseCase"
               id="reprotool.uml.export.action1"
               label="Convert SW project to UML use case diagram">
         </action>
      </objectContribution>
   </extension>

</plugin>
