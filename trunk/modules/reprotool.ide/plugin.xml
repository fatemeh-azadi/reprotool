<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         point="org.eclipse.ui.views">
      <category
            id="cz.cuni.mff.reprotool.ide.category_reprotool"
            name="ReProTool">
      </category>
      <view
         name="Progress View"
         category="org.eclipse.ui"
         class="org.eclipse.ui.ExtensionFactory:progressView"
         id="org.eclipse.ui.views.ProgressView"
         restorable="true">
      </view>
      <view
            class="reprotool.ide.views.OutlineContents"
            id="reprotool.ide.views.OutlineContents"
            name="Outline View">
      </view>
      <view
            category="cz.cuni.mff.reprotool.ide.category_reprotool"
            class="reprotool.ide.views.sentenceanalysis.step.SentenceAnalysisSheet"
            id="reprotool.ide.views.sentenceanalysis.SentenceAnalysisSheet"
            name="Sentence analysis">
      </view>
   </extension>
   <extension
         point="org.eclipse.ui.perspectives">
      <perspective
            class="reprotool.ide.ReprotoolIdePerspectiveFactory"
            icon="icons/reprotool-web-logo-16x16.png"
            id="cz.cuni.mff.reprotool.ide.perspective_reprotool_ide"
            name="Reprotool">
      </perspective>
   </extension>
   <extension
         point="org.eclipse.ui.editors">
      <editor
            class="reprotool.ide.editors.project.ProjectEditor"
            contributorClass="reprotool.ide.editors.project.ProjectActionBarContributor"
            default="true"
            extensions="swproj"
            filenames="swproj"
            icon="icons/book.png"
            id="cz.cuni.mff.reprotool.ide.editors.ProjectEditor"
            name="Project editor">
      </editor>
      <editor
            class="reprotool.ide.editors.usecase.UsecaseEMFEditor"
            contributorClass="reprotool.ide.editors.usecase.UsecaseEMFActionBarContributor"
            default="false"
            extensions="usecase"
            icon="icons/use-case-icon-16x16.png"
            id="org.eclipselabs.reprotool.ide.UseCaseEmfEditor"
            name="Use case EMF Editor">
      </editor>
      <editor
            class="reprotool.ide.editors.counterExample.CExmpEditor"
            contributorClass="reprotool.model.swproj.presentation.SwprojActionBarContributor"
            default="true"
            extensions="cexmp"
            filenames="cexmp"
            id="reprotool.ide.counterExampleEditorId"
            name="Counter example editor">
      </editor>
   </extension>
   <extension
         point="org.eclipse.ui.newWizards">
      <category
            id="reprotool.ide.category1"
            name="Reprotool">
      </category>
      <wizard
            category="reprotool.ide.category1"
            class="reprotool.ide.wizards.ProjectWizard"
            finalPerspective="cz.cuni.mff.reprotool.ide.perspective_reprotool_ide"
            hasPages="true"
            icon="icons/reprotool-web-logo-16x16.png"
            id="reprotool.ide.wizard1"
            name="Reprotool Project"
            project="true">
      </wizard>
   </extension>
   <extension
         point="org.eclipse.ui.preferencePages">
      <page
            class="reprotool.ide.preferences.ReprotoolPreferencePage"
            id="reprotool.ide.preferences.ReprotoolPreferencePage"
            name="ReProTool">
      </page>
   </extension>
   <extension
         point="org.eclipse.core.runtime.preferences">
      <initializer
            class="reprotool.ide.preferences.PreferenceInitializer">
      </initializer>
      <initializer
            class="reprotool.ide.preferences.PreferenceInitializer">
      </initializer>
   </extension>
   <extension
         point="org.eclipse.ui.perspectiveExtensions">
      <perspectiveExtension
            targetID="cz.cuni.mff.reprotool.ide.perspective_reprotool_ide">
         <viewShortcut
               id="cz.cuni.mff.reprotool.ide.views.view_linguistic_tools">
         </viewShortcut>
         <viewShortcut
               id="cz.cuni.mff.reprotool.ide.view_project">
         </viewShortcut>
         <view
               closeable="true"
               id="org.eclipse.ui.navigator.ProjectExplorer"
               minimized="false"
               moveable="true"
               ratio="0.25"
               relationship="left"
               relative="org.eclipse.ui.editorss"
               visible="true">
         </view>
         <view
               closeable="true"
               id="org.eclipse.ui.views.PropertySheet"
               minimized="false"
               moveable="true"
               ratio="0.6"
               relationship="right"
               relative="org.eclipse.ui.editorss"
               visible="true">
         </view>
         <view
               closeable="true"
               id="org.eclipse.ui.views.ProgressView"
               minimized="false"
               moveable="true"
               ratio="0.7"
               relationship="bottom"
               relative="org.eclipse.ui.editorss"
               visible="true">
         </view>
         <view
               closeable="true"
               id="cz.cuni.mff.reprotool.ide.views.view_linguistic_tools"
               minimized="false"
               moveable="true"
               relationship="stack"
               relative="org.eclipse.ui.views.ProgressView"
               visible="true">
         </view>
         <view
               closeable="true"
               id="cz.cuni.mff.reprotool.ide.view_parsed_tree"
               minimized="false"
               moveable="true"
               relationship="bottom"
               relative="org.eclipse.ui.views.PropertySheet"
               visible="true">
         </view>
         <newWizardShortcut
               id="reprotool.ide.wizard1">
         </newWizardShortcut>
         <view
               closeable="true"
               id="org.eclipse.ui.views.ContentOutline"
               minimized="false"
               moveable="true"
               relationship="bottom"
               relative="org.eclipse.ui.views.PropertySheet"
               visible="true">
         </view>
      </perspectiveExtension>
   </extension>
   <extension
         id="reprotool.ide.natures.ReprotoolProjectNature"
         name="Reprotool Nature"
         point="org.eclipse.core.resources.natures">
      <runtime>
         <run
               class="reprotool.ide.natures.ReprotoolProjectNature">
         </run>
      </runtime>
   </extension>
   <extension
         point="org.eclipse.ui.ide.projectNatureImages">
      <image
            icon="icons/reprotool-web-logo-16x16.png"
            id="reprotool.ide.natures.ProjectIcon"
            natureId="reprotool.ide.natures.ReprotoolProjectNature">
      </image>
   </extension>
   <extension
         point="org.eclipse.core.runtime.adapters">
      <factory
            adaptableType="reprotool.model.usecase.UseCase"
            class="reprotool.ide.adapter.UseCaseAdapterFactory">
         <adapter
               type="org.eclipse.ui.views.contentoutline.IContentOutlinePage">
         </adapter>
      </factory>
   </extension>

</plugin>
