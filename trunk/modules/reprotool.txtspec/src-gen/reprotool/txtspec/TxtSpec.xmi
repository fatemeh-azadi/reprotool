<?xml version="1.0" encoding="ASCII"?>
<xtext:Grammar xmi:version="2.0" xmlns:xmi="http://www.omg.org/XMI" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:ecore="http://www.eclipse.org/emf/2002/Ecore" xmlns:xtext="http://www.eclipse.org/2008/Xtext" name="reprotool.txtspec.TxtSpec" definesHiddenTokens="true" hiddenTokens="//@rules.6">
  <metamodelDeclarations xsi:type="xtext:GeneratedMetamodel" name="txtSpec">
    <ePackage href="http://eclipselabs.org/reprotool/txtspec#/"/>
  </metamodelDeclarations>
  <metamodelDeclarations xsi:type="xtext:ReferencedMetamodel" alias="ecore">
    <ePackage href="http://www.eclipse.org/emf/2002/Ecore#/"/>
  </metamodelDeclarations>
  <rules xsi:type="xtext:ParserRule" name="Document">
    <type metamodel="//@metamodelDeclarations.0">
      <classifier xsi:type="ecore:EClass" href="http://eclipselabs.org/reprotool/txtspec#//Document"/>
    </type>
    <alternatives xsi:type="xtext:Assignment" cardinality="*" feature="blocks" operator="+=">
      <terminal xsi:type="xtext:RuleCall" rule="//@rules.1"/>
    </alternatives>
  </rules>
  <rules xsi:type="xtext:ParserRule" name="Block">
    <type metamodel="//@metamodelDeclarations.0">
      <classifier xsi:type="ecore:EClass" href="http://eclipselabs.org/reprotool/txtspec#//Block"/>
    </type>
    <alternatives xsi:type="xtext:Alternatives">
      <elements xsi:type="xtext:RuleCall" rule="//@rules.5"/>
      <elements xsi:type="xtext:RuleCall" rule="//@rules.2"/>
    </alternatives>
  </rules>
  <rules xsi:type="xtext:ParserRule" name="ModelElementBlock">
    <type metamodel="//@metamodelDeclarations.0">
      <classifier xsi:type="ecore:EClass" href="http://eclipselabs.org/reprotool/txtspec#//ModelElementBlock"/>
    </type>
    <alternatives xsi:type="xtext:Group">
      <elements xsi:type="xtext:Keyword" value="("/>
      <elements xsi:type="xtext:Assignment" feature="url" operator="=">
        <terminal xsi:type="xtext:RuleCall" rule="//@rules.4"/>
      </elements>
      <elements xsi:type="xtext:Assignment" feature="content" operator="=">
        <terminal xsi:type="xtext:RuleCall" rule="//@rules.3"/>
      </elements>
      <elements xsi:type="xtext:Keyword" value=")"/>
    </alternatives>
  </rules>
  <rules xsi:type="xtext:ParserRule" name="ModelElementBlockContent">
    <type metamodel="//@metamodelDeclarations.0">
      <classifier xsi:type="ecore:EClass" href="http://eclipselabs.org/reprotool/txtspec#//ModelElementBlockContent"/>
    </type>
    <alternatives xsi:type="xtext:Group">
      <elements xsi:type="xtext:Action">
        <type metamodel="//@metamodelDeclarations.0">
          <classifier xsi:type="ecore:EClass" href="http://eclipselabs.org/reprotool/txtspec#//ModelElementBlockContent"/>
        </type>
      </elements>
      <elements xsi:type="xtext:Assignment" cardinality="*" feature="blocks" operator="+=">
        <terminal xsi:type="xtext:RuleCall" rule="//@rules.1"/>
      </elements>
    </alternatives>
  </rules>
  <rules xsi:type="xtext:ParserRule" name="UrlParam">
    <type metamodel="//@metamodelDeclarations.1">
      <classifier xsi:type="ecore:EDataType" href="http://www.eclipse.org/emf/2002/Ecore#//EString"/>
    </type>
    <alternatives xsi:type="xtext:RuleCall" rule="//@rules.7"/>
  </rules>
  <rules xsi:type="xtext:ParserRule" name="TextBlock">
    <type metamodel="//@metamodelDeclarations.0">
      <classifier xsi:type="ecore:EClass" href="http://eclipselabs.org/reprotool/txtspec#//TextBlock"/>
    </type>
    <alternatives xsi:type="xtext:Assignment" cardinality="+" predicated="true" feature="words" operator="+=">
      <terminal xsi:type="xtext:RuleCall" rule="//@rules.7"/>
    </alternatives>
  </rules>
  <rules xsi:type="xtext:TerminalRule" name="WS">
    <type metamodel="//@metamodelDeclarations.1">
      <classifier xsi:type="ecore:EDataType" href="http://www.eclipse.org/emf/2002/Ecore#//EString"/>
    </type>
    <alternatives xsi:type="xtext:Alternatives" cardinality="+">
      <elements xsi:type="xtext:Keyword" value=" "/>
      <elements xsi:type="xtext:Keyword" value="&#x9;"/>
      <elements xsi:type="xtext:Keyword" value="&#xD;"/>
      <elements xsi:type="xtext:Keyword" value="&#xA;"/>
    </alternatives>
  </rules>
  <rules xsi:type="xtext:TerminalRule" name="WORD">
    <type metamodel="//@metamodelDeclarations.1">
      <classifier xsi:type="ecore:EDataType" href="http://www.eclipse.org/emf/2002/Ecore#//EString"/>
    </type>
    <alternatives xsi:type="xtext:Alternatives" cardinality="+">
      <elements xsi:type="xtext:CharacterRange">
        <left value="a"/>
        <right value="z"/>
      </elements>
      <elements xsi:type="xtext:CharacterRange">
        <left value="A"/>
        <right value="Z"/>
      </elements>
      <elements xsi:type="xtext:CharacterRange">
        <left value="0"/>
        <right value="9"/>
      </elements>
      <elements xsi:type="xtext:Keyword" value=","/>
      <elements xsi:type="xtext:Keyword" value="@"/>
      <elements xsi:type="xtext:Keyword" value="."/>
      <elements xsi:type="xtext:Keyword" value="&amp;"/>
      <elements xsi:type="xtext:Keyword" value=":"/>
      <elements xsi:type="xtext:Keyword" value="/"/>
      <elements xsi:type="xtext:Keyword" value="?"/>
      <elements xsi:type="xtext:Keyword" value="%"/>
      <elements xsi:type="xtext:Keyword" value=";"/>
      <elements xsi:type="xtext:Keyword" value="-"/>
    </alternatives>
  </rules>
</xtext:Grammar>
